{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [
      "error",
      "double"
    ]
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "es6": true,
    "ecmaVersion": 2017
  }
}